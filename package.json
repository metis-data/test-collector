{
  "name": "test-collector",
  "version": "1.0.0",
  "description": "Set up metis otel collector",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run format && npm run clean && npm run build && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metis-data/test-collector.git"
  },
  "keywords": [
    "github",
    "actions",
    "metis",
    "otel"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metis-data/test-collector/issues"
  },
  "homepage": "https://github.com/metis-data/test-collector#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@actions/http-client": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
